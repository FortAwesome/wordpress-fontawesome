{
    "name": "fortawesome/wordpress-fontawesome",
    "description": "Official Font Awesome WordPress plugin composer package.",
    "keywords": [ "font awesome", "font-awesome", "icon", "icons", "webfont", "svg" ],
    "homepage": "https://fontawesome.com",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Mike Wilkerson",
            "email": "mwlkerson@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "hello@fontawesome.com"
    },
    "minimum-stability": "beta",
    "archive": {
        "exclude": [
            "/images",
            "/integrations",
            "/tests",
            ".gitattributes",
            ".github",
            ".gitignore",
            ".travis.yml",
            "/images",
            "phpunit.xml.dist",
            "phpcs.xml.dist",
            "/.git",
            "proxy-webpack-dev.conf",
            "readme.txt",
            "docker-php-ext-xdebug.ini",
            "docker-compose.yml",
            "/bin",
            "admin/config",
            "admin/src",
            "admin/scripts",
            "Dockerfile",
            ".env",
            "DEVELOPMENT.md"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4.5",
        "yoast/phpunit-polyfills": "^1.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
