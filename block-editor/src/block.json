{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "font-awesome/icon",
  "version": "0.1.0",
  "title": "Font Awesome Icon",
  "category": "widgets",
  "description": "Add a Font Awesome icon.",
  "example": {},
  "attributes": {
    "prefix": {
      "type": "string"
    },
    "iconName": {
      "type": "string"
    },
    "width": {
      "type": "integer"
    },
    "height": {
      "type": "integer"
    },
    "transform": {
      "type": "string"
    },
    "primaryPath": {
      "type": "string"
    },
    "secondaryPath": {
      "type": "string"
    },
    "spin": {
      "type": "boolean"
    }
  },
  "supports": {
    "anchor": true,
    "align": ["left", "center", "right" ],
    "ariaLabel": true,
    "color": {
      "background": true,
      "text": true,
      "enableConstrastChecker": true,
      "gradients": true
    },
    "dimensions": {
      "aspectRatio": true,
      "minHeight": true
    },
    "html": false,
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    }
  },
  "textdomain": "fa-icon-block",
  "editorScript": "font-awesome-block-editor-support",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
