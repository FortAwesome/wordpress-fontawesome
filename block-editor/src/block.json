{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "font-awesome/icon",
  "version": "0.1.0",
  "title": "Font Awesome Icon",
  "category": "design",
  "description": "Add a Font Awesome icon.",
  "example": {},
  "attributes": {
    "iconLayers": {
      "type": "array"
    }
  },
  "supports": {
    "anchor": true,
    "align": ["left", "center", "right"],
    "ariaLabel": true,
    "color": {
      "background": true,
      "text": true,
      "enableConstrastChecker": true,
      "gradients": true
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "html": false,
    "typography": {
      "fontSize": true,
      "lineHeight": true
    }
  },
  "textdomain": "fa-icon-block",
  "editorScript": "font-awesome-block-editor-support",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
