{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "font-awesome/icon",
  "version": "0.1.0",
  "title": "Font Awesome Icon",
  "category": "widgets",
  "description": "Add a Font Awesome icon.",
  "example": {},
  "attributes": {
    "prefix": {
      "type": "string"
    },
    "iconName": {
      "type": "string"
    },
    "width": {
      "type": "integer"
    },
    "height": {
      "type": "integer"
    },
    "pathData": {
      "type": "array"
    },
    "transform": {
      "type": "string"
    },
    "fixedWidth": {
      "type": "boolean"
    },
    "rotate": {
      "type": "integer"
    },
    "flip": {
      "enum": ["horizontal", "vertical", "both"]
    },
    "bounce": {
      "type": "boolean"
    },
    "beat": {
      "type": "boolean"
    },
    "fade": {
      "type": "boolean"
    },
    "beatFade": {
      "type": "boolean"
    },
    "spin": {
      "type": "boolean"
    },
    "spinReverse": {
      "type": "boolean"
    },
    "spinPulse": {
      "type": "boolean"
    },
    "shake": {
      "type": "boolean"
    },
    "border": {
      "type": "boolean"
    },
    "inverse": {
      "type": "boolean"
    }
  },
  "supports": {
    "anchor": true,
    "align": ["left", "center", "right"],
    "ariaLabel": true,
    "color": {
      "background": true,
      "text": true,
      "enableConstrastChecker": true,
      "gradients": true
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "html": false,
    "typography": {
      "fontSize": true,
      "lineHeight": true
    }
  },
  "textdomain": "fa-icon-block",
  "editorScript": "font-awesome-block-editor-support",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
